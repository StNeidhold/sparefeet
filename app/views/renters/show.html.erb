<div class="page-header">
  <h3>Renter #<%= @renter.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Email</dt>
      <dd><%= @renter.email %></dd>

      <dt>Password</dt>
      <dd><%= @renter.password %></dd>

      <dt>First name</dt>
      <dd><%= @renter.first_name %></dd>

      <dt>Last name</dt>
      <dd><%= @renter.last_name %></dd>

      <dt>Telephone</dt>
      <dd><%= @renter.telephone %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/renters" class="btn btn-primary">
        Back
      </a>
    </div>
  </div>
</div>
